b = ['a','b','c','d','e','f','g']
print(b)
['a', 'b', 'c', 'd', 'e', 'f', 'g']
del b[1]
print(b)
['a', 'c', 'd', 'e', 'f', 'g']
del b[2:4]
print(b)
['a', 'c', 'f', 'g']

